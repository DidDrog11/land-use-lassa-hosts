	pmcid = {PMC9430508},
	keywords = {Animals, Humans, Rats, Prevalence, Lassa virus, Lassa fever, Nigeria, Lassa Fever, Murinae, Epidemics, infected tissues, Lassa diagnosis, small rodents},
