	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/1749-4877.12149},
	keywords = {rodent-borne diseases, transmission ecology, dilution effect, biodiversity, multi-infection},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\GKGPA6UL\\Bordes et al. - 2015 - Transmission ecology of rodent-borne diseases New.pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\WQRAC3T8\\1749-4877.html:text/html},
