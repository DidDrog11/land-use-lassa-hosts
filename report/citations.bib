@article{leach_local_2017,
	title = {Local disease–ecosystem–livelihood dynamics: reflections from comparative case studies in Africa},
	volume = {372},
	url = {https://royalsocietypublishing.org/doi/full/10.1098/rstb.2016.0163},
	doi = {10.1098/rstb.2016.0163},
	shorttitle = {Local disease–ecosystem–livelihood dynamics},
	abstract = {This article explores the implications for human health of local interactions between disease, ecosystems and livelihoods. Five interdisciplinary case studies addressed zoonotic diseases in African settings: Rift Valley fever ({RVF}) in Kenya, human African trypanosomiasis in Zambia and Zimbabwe, Lassa fever in Sierra Leone and henipaviruses in Ghana. Each explored how ecological changes and human–ecosystem interactions affect pathogen dynamics and hence the likelihood of zoonotic spillover and transmission, and how socially differentiated peoples’ interactions with ecosystems and animals affect their exposure to disease. Cross-case analysis highlights how these dynamics vary by ecosystem type, across a range from humid forest to semi-arid savannah; the significance of interacting temporal and spatial scales; and the importance of mosaic and patch dynamics. Ecosystem interactions and services central to different people's livelihoods and well-being include pastoralism and agro-pastoralism, commercial and subsistence crop farming, hunting, collecting food, fuelwood and medicines, and cultural practices. There are synergies, but also tensions and trade-offs, between ecosystem changes that benefit livelihoods and affect disease. Understanding these can inform ‘One Health’ approaches towards managing ecosystems in ways that reduce disease risks and burdens.

This article is part of the themed issue ‘One Health for a changing world: zoonoses, ecosystems and human well-being’.},
	pages = {20160163},
	number = {1725},
	journaltitle = {Philosophical Transactions of the Royal Society B: Biological Sciences},
	author = {Leach, Melissa and Bett, Bernard and Said, M. and Bukachi, Salome and Sang, Rosemary and Anderson, Neil and Machila, Noreen and Kuleszo, Joanna and Schaten, Kathryn and Dzingirai, Vupenyu and Mangwanya, Lindiwe and Ntiamoa-Baidu, Yaa and Lawson, Elaine and Amponsah-Mensah, Kofi and Moses, Lina M. and Wilkinson, Annie and Grant, Donald S. and Koninga, James},
	urldate = {2023-06-06},
	date = {2017-06-05},
	note = {Publisher: Royal Society},
	keywords = {zoonosis, Africa, disease, ecosystem, livelihoods},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\7CB4V6RQ\\Leach et al. - 2017 - Local disease–ecosystem–livelihood dynamics refle.pdf:application/pdf;Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\3JY3S3ZB\\Leach et al. - 2017 - Local disease–ecosystem–livelihood dynamics refle.pdf:application/pdf},
}

@article{mulungu_population_2013,
	title = {Population dynamics and breeding patterns of multimammate mouse, Mastomys natalensis (Smith 1834), in irrigated rice fields in Eastern Tanzania},
	volume = {69},
	rights = {Copyright © 2012 Society of Chemical Industry},
	issn = {1526-4998},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ps.3346},
	doi = {10.1002/ps.3346},
	abstract = {{BACKGROUND}: Multimammate mice are the most important vertebrate pests in Sub-Saharan Africa and are also reservoirs of many zoonotic diseases, including sylvan plague. This study investigated the population dynamics and breeding patterns of this mouse in irrigated rice cropping systems in eastern Tanzania. {RESULTS}: The multimammate mouse, Mastomys natalensis, population varied with habitat and months. Fallow land had a more abundant population than rice fields. The highest population peak was observed during the dry season from July to October. Mastomys natalensis is sexually active throughout the year in the study area, although it reaches the highest level in June and December when rice is at the maturity stage. This suggests that breeding is highly influenced by the presence of a rice crop in both seasons. More juvenile individuals were recorded in August and September, indicating that they were produced in the previous breeding months. The sex ratio of M. natalensis was not skewed to either males or females, indicating that it was at parity. {CONCLUSION}: Rodent population dynamics during the study periods in all habitats indicated that high birth rates accounted for the rapid population growth and turnover. Regular control and sustainable operations are thus essential if rodent pest populations are to be kept within tolerable limits. © 2012 Society of Chemical Industry},
	pages = {371--377},
	number = {3},
	journaltitle = {Pest Management Science},
	author = {Mulungu, Loth S and Ngowo, Victoria and Mdangi, Mashaka and Katakweba, Abdul S and Tesha, Protas and Mrosso, Furaha P and Mchomvu, Mary and Sheyo, Paul M and Kilonzo, Bukhet S},
	urldate = {2023-06-06},
	date = {2013},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ps.3346},
	keywords = {rodents, population, breeding, fallow land, rice fields, sex ratio},
	file = {Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\CUQ329YS\\ps.html:text/html},
