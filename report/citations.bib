	urldate = {2023-06-05},
	date = {2021-10-11},
	langid = {english},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\7WDX6CGQ\\Glidden et al. - 2021 - Human-mediated impacts on biodiversity and the con.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\V4SM4U9V\\S0960982221012069.html:text/html},
