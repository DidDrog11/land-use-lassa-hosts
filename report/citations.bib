	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1523-1739.2008.00951.x},
	keywords = {climate change, invasive species, cambio climático, especies invasoras, invasion pathway, invasive-species management, manejo de especies invasoras, vía de invasión},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\YSCGCY3T\\Hellmann et al. - 2008 - Five Potential Consequences of Climate Change for .pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\S8QWSHWX\\j.1523-1739.2008.00951.html:text/html},
