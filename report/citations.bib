	title = {Conservation and people: Towards an ethical code of conduct for the use of camera traps in wildlife research},
	volume = {1},
	rights = {© 2020 The Authors. Ecological Solutions and Evidence published by John Wiley \& Sons Ltd on behalf of British Ecological Society},
